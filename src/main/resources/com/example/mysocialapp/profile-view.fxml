<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600" prefWidth="334.0" stylesheets="@../../../map/css/profile.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mysocialapp.ProfileController">
    <children>
        <VBox layoutX="67.0" layoutY="92.0">
            <ImageView fx:id="profileImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../../map/Images/b.jpg"/>
            </ImageView>
            <Label fx:id="profileFirstName" text="First Name:">
            </Label>
            <Label fx:id="profileSecondName" text="Last Name:">
            </Label>
            <Label fx:id="profileMail" text="Mail:">
            </Label>
        </VBox>
        <Button fx:id="profileMyFriends" layoutX="135.0" layoutY="371.0" mnemonicParsing="false"
                onAction="#onMyFriendsButton" text="👨‍👩‍👦‍👦">
        </Button>
        <Button fx:id="profileAllUsers" layoutX="135.0" layoutY="416.0" mnemonicParsing="false"
                onAction="#onAllUsersButton" text="🔍">
        </Button>
        <Button fx:id="profileNews" layoutX="135.0" layoutY="459.0" mnemonicParsing="false" onAction="#onProfileNews"
                text="📥">
        </Button>
        <Button fx:id="signOut" layoutX="135.0" layoutY="504.0" mnemonicParsing="false" onAction="#onSignOut"
                text="📴"/>
        <ImageView fitHeight="67.0" fitWidth="353.0" layoutX="4.0" layoutY="-3.0" pickOnBounds="true"
                   preserveRatio="true">
        </ImageView>
        <ImageView fitHeight="67.0" fitWidth="353.0" layoutX="4.0" layoutY="569.0" pickOnBounds="true"
                   preserveRatio="true">

        </ImageView>
    </children>
</AnchorPane>
